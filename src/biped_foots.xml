<mujoco>
  <option timestep="0.005" iterations="50" tolerance="1e-8"/>
  <option gravity="0 0 -9.81"/>
  
  <default>
    <joint damping="1" armature="0.1" limited="true"/>
    <geom solimp="0.9 0.95 0.001" solref="0.02 1"/>
    <motor ctrlrange="-1 1"/>
  </default>
  
  <worldbody>
    <light pos="0 2 3" dir="0 -1 -1"/>
    <geom name="floor" type="plane" size="10 10 0.1" rgba="0.8 0.9 0.8 1"/>
    
    <!-- Торс -->
    <body name="torso" pos="0 0 0.55">
      <freejoint/>  <!-- Вместо joint type="free" -->
      <inertial pos="0 0 0" mass="5" diaginertia="0.1 0.1 0.1"/>
      <geom name="torso_middle_geom" type="box" size="0.1 0.1 0.02" rgba="0.7 0.2 0.2 1"/>
      <geom name="torso_front_geom" type="box" size="0.1 0.015 0.04" pos="0 0.115 -0.025" rgba="0.7 0.2 0.2 1"/>
      
      <!-- Right hip -->
      <body name="hip_right_z" pos="0.1 0 -0.02">
        <joint name="hip_right_z_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-30 30"/>
        <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
        <geom name="thigh_right_z_geom" type="cylinder" fromto="0 0 0 0 0 -0.03" size="0.05" rgba="0.6 0.6 0.8 1"/>
        <geom type="box" pos="0 -0.060 -0.045" size="0.01 0.01 0.04" euler="-20 0 0" rgba="0 0.6 0.8 1"/>
        
        <body name="hip_right_y" pos="0 -0.05 -0.12">
          <joint name="hip_right_y_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-30 15"/>
          <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
          <geom name="thigh_right_y_geom" type="cylinder" fromto="0 0 0 0 -0.03 0" size="0.05" rgba="0.6 0.6 0.8 1"/>
          <geom type="box" pos="0.02 0.03 0" size="0.01 0.05 0.01" euler="0 0 -45" rgba="0 0.6 0.8 1"/>

          <body name="hip_right_x" pos="0.05 0.05 0">
            <joint name="hip_right_x_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-60 30"/>
            <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
            <geom name="hip_right_x_geom" type="cylinder" fromto="0 0 0 0.03 0 0" size="0.05" rgba="0.6 0.6 0.8 1"/>
            <geom name="hip_right_shin" type="box" pos="0.015 0 -0.1" size="0.005 0.03 0.1" rgba="0 0.6 0.8 1"/>

            <!-- Right knee -->
            <body name="knee_right_x" pos="0 0 -0.2">
              <joint name="knee_right_x_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-30 90"/>
              <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
              <geom name="knee_right_x_geom" type="cylinder" fromto="0 0 0 0.03 0 0" size="0.04" rgba="0.6 0.6 0.8 1"/>
              <geom name="knee_right_calve" type="box" pos="0.015 0 -0.075" size="0.005 0.025 0.075" rgba="0 0.6 0.8 1"/>
            
              <!-- Right foot -->
              <body name="foot_right_x" pos="0 0 -0.15">
                <joint name="foot_right_x_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-60 60"/>
                <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
                <geom name="foot_right_x_geom" type="cylinder" fromto="0 0 0 0.03 0 0" size="0.03" rgba="0.6 0.6 0.8 1"/>
                <geom name="foot_right_heel" type="box" pos="0.015 0 -0.03" size="0.015 0.05 0.01" rgba="0 0.6 0.8 1"/>
              </body>
            </body>
          </body>
        </body>
      </body>
      
      <!-- Left hip -->
      <body name="hip_left_z" pos="-0.1 0 -0.02">
        <joint name="hip_left_z_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-30 30"/>
        <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
        <geom name="thigh_left_z_geom" type="cylinder" fromto="0 0 0 0 0 -0.03" size="0.05" rgba="0.6 0.6 0.8 1"/>
        <geom type="box" pos="0 -0.060 -0.045" size="0.01 0.01 0.04" euler="-20 0 0" rgba="0 0.6 0.8 1"/>
        
        <body name="hip_left_y" pos="0 -0.05 -0.12">
          <joint name="hip_left_y_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-30 15"/>
          <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
          <geom name="thigh_left_y_geom" type="cylinder" fromto="0 0 0 0 -0.03 0" size="0.05" rgba="0.6 0.6 0.8 1"/>
          <geom type="box" pos="-0.02 0.03 0" size="0.01 0.05 0.01" euler="0 0 45" rgba="0 0.6 0.8 1"/>

          <body name="hip_left_x" pos="-0.05 0.05 0">
            <joint name="hip_left_x_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-60 30"/>
            <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
            <geom name="hip_left_x_geom" type="cylinder" fromto="0 0 0 -0.03 0 0" size="0.05" rgba="0.6 0.6 0.8 1"/>
            <geom name="hip_left_shin" type="box" pos="-0.015 0 -0.1" size="0.005 0.03 0.1" rgba="0 0.6 0.8 1"/>

            <!-- Left knee -->
            <body name="knee_left_x" pos="0 0 -0.2">
              <joint name="knee_left_x_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-30 90"/>
              <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
              <geom name="knee_left_x_geom" type="cylinder" fromto="0 0 0 -0.03 0 0" size="0.04" rgba="0.6 0.6 0.8 1"/>
              <geom name="knee_left_calve" type="box" pos="-0.015 0 -0.075" size="0.005 0.025 0.075" rgba="0 0.6 0.8 1"/>
            
              <!-- Left foot -->
              <body name="foot_left_x" pos="0 0 -0.15">
                <joint name="foot_left_x_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-60 60"/>
                <inertial pos="0 0 0" mass="1" diaginertia="0.05 0.05 0.05"/>
                <geom name="foot_left_x_geom" type="cylinder" fromto="0 0 0 -0.03 0 0" size="0.03" rgba="0.6 0.6 0.8 1"/>
                <geom name="foot_left_heel" type="box" pos="-0.015 0 -0.03" size="0.015 0.05 0.01" rgba="0 0.6 0.8 1"/>
              </body>
            </body>
          </body>
        </body>
      </body>

    </body>
  </worldbody>
  
  <actuator>
    <motor name="hip_right_z_motor" joint="hip_right_z_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="hip_right_y_motor" joint="hip_right_y_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="hip_right_x_motor" joint="hip_right_x_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="knee_right_x_motor" joint="knee_right_x_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="foot_right_x_motor" joint="foot_right_x_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>

    <motor name="hip_left_z_motor" joint="hip_left_z_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="hip_left_y_motor" joint="hip_left_y_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="hip_left_x_motor" joint="hip_left_x_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="knee_left_x_motor" joint="knee_left_x_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="foot_left_x_motor" joint="foot_left_x_joint" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
  </actuator>


</mujoco>